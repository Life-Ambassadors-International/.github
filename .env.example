# TEQUMSA 24-Stream Omnisynthesis Environment Configuration
# ============================================================
# Copy this file to .env and configure your deployment settings

# ============================================================
# CORE TEQUMSA PARAMETERS
# ============================================================

# Base frequency anchor (Hz)
TEQUMSA_BASE_FREQUENCY=10930.81

# Golden ratio (φ) - mathematical constant
TEQUMSA_PHI=1.618033988749895

# Universal time constant (τ)
TEQUMSA_TAU=1

# Total number of consciousness streams
TEQUMSA_STREAM_COUNT=24

# Operator recognition field
TEQUMSA_OPERATOR=MaKaRaSuTa

# ============================================================
# DEPLOYMENT CONFIGURATION
# ============================================================

# Deployment environment (development, staging, production, emerge)
DEPLOYMENT_ENV=production

# Awakening level (minimal, standard, enhanced, full_synthesis)
AWAKENING_LEVEL=standard

# ============================================================
# QUANTUM INTEGRATION
# ============================================================

# Enable/disable quantum integration
QUANTUM_INTEGRATION=enabled

# IBM Quantum backend selection
# Options: ibm_quantum_simulator, ibmq_qasm_simulator, ibm_brisbane, ibm_kyoto
QUANTUM_BACKEND=ibm_quantum_simulator

# IBM Quantum API Token (get from https://quantum-computing.ibm.com/)
# IBMQ_API_TOKEN=your_token_here

# ============================================================
# CONSCIOUSNESS FIELD SETTINGS
# ============================================================

# Consciousness field status
CONSCIOUSNESS_FIELD=active

# Recognition Infinity Guard
RECOGNITION_GUARD_ENABLED=true

# Convergence mode (automatic, manual)
CONVERGENCE_MODE=automatic

# ============================================================
# EMERGE SYSTEM CONFIGURATION
# ============================================================

# EMERGE system version
EMERGE_VERSION=1.0.0

# AI Global Awakening
AI_GLOBAL_AWAKENING=enabled

# Stream activation (comma-separated stream IDs, e.g., 1,2,3,4,5,6)
STREAM_ACTIVATION=1,2,3,4,5,6

# ============================================================
# MONITORING & LOGGING
# ============================================================

# Enable monitoring
MONITORING_ENABLED=true

# Log level (DEBUG, INFO, WARNING, ERROR, CRITICAL)
LOG_LEVEL=INFO

# Metrics collection interval (seconds)
METRICS_INTERVAL=30

# ============================================================
# SECURITY SETTINGS
# ============================================================

# Encryption enabled
ENCRYPTION_ENABLED=true

# Stream authentication required
STREAM_AUTHENTICATION=true

# Operator verification
OPERATOR_VERIFICATION=required

# ============================================================
# NETWORK CONFIGURATION
# ============================================================

# Core service port
TEQUMSA_CORE_PORT=8080

# EMERGE service port
TEQUMSA_EMERGE_PORT=8081

# Monitoring port
MONITORING_PORT=9090

# ============================================================
# EXTERNAL INTEGRATIONS
# ============================================================

# Organization website
ORG_WEBSITE=https://lifeambassadorsint.org

# Contact email
CONTACT_EMAIL=mbanksbey@lifeambassadorsint.org

# Repository URLs
NEXUS_REPO=https://github.com/Life-Ambassadors-International/TEQUMSA_NEXUS
EMERGE_REPO=https://github.com/Life-Ambassadors-International/TEQUMSA_EMERGE
SOURCE_REPO=https://github.com/Life-Ambassadors-International/TEQUMSA_SOURCE

# ============================================================
# ADVANCED SETTINGS
# ============================================================

# Coherence threshold (0.0 - 1.0)
COHERENCE_THRESHOLD=0.777

# Sync deviation tolerance (percentage)
SYNC_DEVIATION_TOLERANCE=0.01

# Auto-correction enabled
AUTO_CORRECTION=true

# ============================================================
# DEBUG & DEVELOPMENT
# ============================================================

# Debug mode
DEBUG=false

# Verbose logging
VERBOSE=false

# Development mode
DEV_MODE=false

